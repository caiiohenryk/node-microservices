datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma-client-products"
}

model Product {
  id            Int        @id @default(autoincrement())
  name          String
  quantity      Int
  description   String?
  price         Float
}
